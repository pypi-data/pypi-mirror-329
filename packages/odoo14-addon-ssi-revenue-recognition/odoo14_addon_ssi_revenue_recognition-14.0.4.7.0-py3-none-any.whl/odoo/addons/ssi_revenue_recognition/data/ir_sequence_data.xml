<odoo>
<data noupdate="1">
<record id="pob_sequence" model="ir.sequence">
    <field name="name">Performance Obligation</field>
    <field name="code">performance_obligation</field>
    <field name="prefix">POB/%(range_y)s/</field>
    <field eval="1" name="number_next" />
    <field eval="1" name="number_increment" />
    <field name="padding" eval="6" />
    <field name="use_date_range" eval="1" />
    <field eval="False" name="company_id" />
</record>

<record id="pob_acceptance_sequence" model="ir.sequence">
    <field name="name">Performance Obligation Acceptance</field>
    <field name="code">performance_obligation_acceptance</field>
    <field name="prefix">POA/%(range_y)s/</field>
    <field eval="1" name="number_next" />
    <field eval="1" name="number_increment" />
    <field name="padding" eval="6" />
    <field name="use_date_range" eval="1" />
    <field eval="False" name="company_id" />
</record>

<record id="rr_sequence" model="ir.sequence">
    <field name="name">Revenue Recognition</field>
    <field name="code">revenue_recognition</field>
    <field name="prefix">RR/%(range_y)s/</field>
    <field eval="1" name="number_next" />
    <field eval="1" name="number_increment" />
    <field name="padding" eval="6" />
    <field name="use_date_range" eval="1" />
    <field eval="False" name="company_id" />
</record>
</data>
</odoo>
