{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "contactDataSet": {
      "type": "object",
      "properties": {
        "@xmlns:common": {
          "type": "string"
        },
        "@xmlns": {
          "type": "string"
        },
        "@xmlns:xsi": {
          "type": "string"
        },
        "@version": {
          "type": "string"
        },
        "@xsi:schemaLocation": {
          "type": "string"
        },
        "contactInformation": {
          "type": "object",
          "properties": {
            "dataSetInformation": {
              "type": "object",
              "properties": {
                "common:UUID": {
                  "type": "string",
                  "format": "uuid"
                },
                "common:shortName": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "@xml:lang": {
                        "type": "string"
                      },
                      "#text": {
                        "type": "string"
                      }
                    },
                    "required": ["@xml:lang", "#text"]
                  }
                },
                "common:name": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "@xml:lang": {
                        "type": "string"
                      },
                      "#text": {
                        "type": "string"
                      }
                    },
                    "required": ["@xml:lang", "#text"]
                  }
                },
                "classificationInformation": {
                  "type": "object",
                  "properties": {
                    "common:classification": {
                      "type": "object",
                      "properties": {
                        "common:class": {
                          "type": "object",
                          "properties": {
                            "@level": {
                              "type": "string"
                            },
                            "#text": {
                              "type": "string"
                            }
                          },
                          "required": ["@level", "#text"]
                        }
                      },
                      "required": ["common:class"]
                    }
                  },
                  "required": ["common:classification"]
                },
                "email": {
                  "type": "string",
                  "format": "email"
                }
              },
              "required": ["common:UUID", "common:shortName", "common:name", "email"]
            }
          },
          "required": ["dataSetInformation"]
        },
        "administrativeInformation": {
          "type": "object",
          "properties": {
            "publicationAndOwnership": {
              "type": "object",
              "properties": {
                "common:dataSetVersion": {
                  "type": "string"
                }
              },
              "required": ["common:dataSetVersion"]
            }
          },
          "required": ["publicationAndOwnership"]
        }
      },
      "required": [
        "@xmlns:common",
        "@xmlns",
        "@xmlns:xsi",
        "@version",
        "@xsi:schemaLocation",
        "contactInformation",
        "administrativeInformation"
      ]
    }
  },
  "required": ["contactDataSet"]
}
