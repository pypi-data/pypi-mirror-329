{
    "name" : "harmonizefullrefresh",
    "label": "ORI to GBD Format without indexing",
    "dag": {
        "nodes": {
            "manifests" : "harmonize.Manifests",
            "etl"       : "harmonize.ETLProcess",
            "notify"    : "harmonize.Notify"
        },
        "edges": [
            {
                "from"     : "manifests",
                "to"       : "etl",
                "bindings" : { "etl.manifest" : "manifests.manifest_list" }
            },
            {
                "from"     : "manifests",
                "to"       : "notify",
                "bindings" : { "notify.manifest" : "manifests.manifest_list"}
            },
            {
                "from"     : "etl",
                "to"       : "notify",
                "bindings" : { "notify.flag" : "index.flag" }
            }
        ]
    },
    "config" : {
        "steps": {
            "index": {
                "uat": 2
            }
        }
    }
}
